{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "how2meet: main",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "how2meet.main:app",
                "--reload",
                "--log-level=debug",
                "--port=8000"
            ],
            "jinja": true,
            "justMyCode": true,
            "console": "integratedTerminal",
            "subProcess": true,
            "serverReadyAction": {
                "pattern": "Uvicorn running on http://127.0.0.1:([0-9]+)",
                "uriFormat": "http://localhost:%s",
                "action": "openExternally"
                    }
        }
    ]
}
